{
  "name": "kbn-alert-load deployment",
  "resources": {
    "elasticsearch": [
      {
        "region": "gcp-us-west2", 
        "ref_id": "main-elasticsearch",
        "plan": {
          "cluster_topology": [
            {
              "instance_configuration_id": "gcp-io-optimized", 
              "zone_count": 1, 
              "size": {
                "resource": "memory",
                "value": 65792 
              }
            }
          ],
          "elasticsearch": {
            "version": "8.1.0-SNAPSHOT"
          },
          "deployment_template": {
            "id": "gcp-io-optimized" 
          }
        }
      }
    ],
    "kibana": [
      {
        "region": "gcp-us-west2",
        "elasticsearch_cluster_ref_id": "main-elasticsearch",
        "ref_id": "main-kibana",
        "plan": {
          "cluster_topology": [
            {
              "instance_configuration_id": "gcp-io-optimized",
              "zone_count": 1, 
              "size": {
                "resource": "memory",
                "value": 16384 
              }
            }
          ],
          "kibana": {
            "docker_image": "docker.elastic.co/kibana/kibana:8.1.0-SNAPSHOT",
            "version": "8.1.0-SNAPSHOT"
          }
        }
      }
    ]
  }
}